<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BCAF"
    tools:context=".MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:layout_margin="10dp">
        <LinearLayout
            android:id="@+id/circle1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:contentPadding="15dp"
                app:cardElevation="2dp"
                app:cardCornerRadius="100dp"
                app:cardBackgroundColor="#CFA6F5">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:contentPadding="15dp"
                    app:cardElevation="10dp"
                    app:cardCornerRadius="100dp"
                    app:cardBackgroundColor="#FFA6F5">
                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:contentPadding="20dp"
                        app:cardElevation="2dp"
                        app:cardCornerRadius="100dp"
                        app:cardBackgroundColor="#FFB6E5">
                        <androidx.cardview.widget.CardView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:contentPadding="20dp"
                            app:cardElevation="2dp"
                            app:cardCornerRadius="100dp"
                            app:cardBackgroundColor="#FFC6D5">
                            <androidx.cardview.widget.CardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:contentPadding="20dp"
                                app:cardElevation="2dp"
                                app:cardCornerRadius="100dp"
                                app:cardBackgroundColor="#FFD6C5">
                                <androidx.cardview.widget.CardView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:contentPadding="20dp"
                                    app:cardElevation="2dp"
                                    app:cardCornerRadius="100dp"
                                    app:cardBackgroundColor="#FFD6B5">
                                    <androidx.cardview.widget.CardView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        app:contentPadding="20dp"
                                        app:cardElevation="2dp"
                                        app:cardCornerRadius="100dp"
                                        app:cardBackgroundColor="#FFE6A5">
                                    </androidx.cardview.widget.CardView>
                                </androidx.cardview.widget.CardView>
                            </androidx.cardview.widget.CardView>
                        </androidx.cardview.widget.CardView>
                    </androidx.cardview.widget.CardView>
                </androidx.cardview.widget.CardView>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:contentPadding="15dp"
                app:cardElevation="2dp"
                app:cardCornerRadius="100dp"
                app:cardBackgroundColor="#CFA6F5">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:contentPadding="15dp"
                    app:cardElevation="10dp"
                    app:cardCornerRadius="100dp"
                    app:cardBackgroundColor="#FFA6F5">
                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:contentPadding="20dp"
                        app:cardElevation="2dp"
                        app:cardCornerRadius="100dp"
                        app:cardBackgroundColor="#FFB6E5">
                        <androidx.cardview.widget.CardView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:contentPadding="20dp"
                            app:cardElevation="2dp"
                            app:cardCornerRadius="100dp"
                            app:cardBackgroundColor="#FFC6D5">
                            <androidx.cardview.widget.CardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:contentPadding="20dp"
                                app:cardElevation="2dp"
                                app:cardCornerRadius="100dp"
                                app:cardBackgroundColor="#FFD6C5">
                                <androidx.cardview.widget.CardView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:contentPadding="20dp"
                                    app:cardElevation="2dp"
                                    app:cardCornerRadius="100dp"
                                    app:cardBackgroundColor="#FFD6B5">
                                    <androidx.cardview.widget.CardView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        app:contentPadding="20dp"
                                        app:cardElevation="2dp"
                                        app:cardCornerRadius="100dp"
                                        app:cardBackgroundColor="#FFE6A5">
                                    </androidx.cardview.widget.CardView>
                                </androidx.cardview.widget.CardView>
                            </androidx.cardview.widget.CardView>
                        </androidx.cardview.widget.CardView>
                    </androidx.cardview.widget.CardView>
                </androidx.cardview.widget.CardView>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/circle2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:gravity="clip_vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:contentPadding="15dp"
                app:cardElevation="2dp"
                app:cardCornerRadius="100dp"
                app:cardBackgroundColor="#CFA6F5">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:contentPadding="15dp"
                    app:cardElevation="10dp"
                    app:cardCornerRadius="100dp"
                    app:cardBackgroundColor="#FFA6F5">
                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:contentPadding="20dp"
                        app:cardElevation="2dp"
                        app:cardCornerRadius="100dp"
                        app:cardBackgroundColor="#FFB6E5">
                        <androidx.cardview.widget.CardView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:contentPadding="20dp"
                            app:cardElevation="2dp"
                            app:cardCornerRadius="100dp"
                            app:cardBackgroundColor="#FFC6D5">
                            <androidx.cardview.widget.CardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:contentPadding="20dp"
                                app:cardElevation="2dp"
                                app:cardCornerRadius="100dp"
                                app:cardBackgroundColor="#FFD6C5">
                                <androidx.cardview.widget.CardView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:contentPadding="20dp"
                                    app:cardElevation="2dp"
                                    app:cardCornerRadius="100dp"
                                    app:cardBackgroundColor="#FFD6B5">
                                    <androidx.cardview.widget.CardView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        app:contentPadding="20dp"
                                        app:cardElevation="2dp"
                                        app:cardCornerRadius="100dp"
                                        app:cardBackgroundColor="#FFE6A5">
                                    </androidx.cardview.widget.CardView>
                                </androidx.cardview.widget.CardView>
                            </androidx.cardview.widget.CardView>
                        </androidx.cardview.widget.CardView>
                    </androidx.cardview.widget.CardView>
                </androidx.cardview.widget.CardView>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/circle3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_margin="10dp">
            <androidx.cardview.widget.CardView
                android:layout_width="200dp"
                android:layout_height="200dp"
                app:contentPadding="15dp"
                app:cardElevation="2dp"
                app:cardCornerRadius="100dp"
                app:cardBackgroundColor="#CFA6F5">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:contentPadding="15dp"
                    app:cardElevation="10dp"
                    app:cardCornerRadius="100dp"
                    app:cardBackgroundColor="#FFA6F5">
                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:contentPadding="20dp"
                        app:cardElevation="2dp"
                        app:cardCornerRadius="100dp"
                        app:cardBackgroundColor="#FFB6E5">
                        <androidx.cardview.widget.CardView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:contentPadding="20dp"
                            app:cardElevation="2dp"
                            app:cardCornerRadius="100dp"
                            app:cardBackgroundColor="#FFC6D5">
                            <androidx.cardview.widget.CardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:contentPadding="20dp"
                                app:cardElevation="2dp"
                                app:cardCornerRadius="100dp"
                                app:cardBackgroundColor="#FFD6C5">
                                <androidx.cardview.widget.CardView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:contentPadding="20dp"
                                    app:cardElevation="2dp"
                                    app:cardCornerRadius="100dp"
                                    app:cardBackgroundColor="#FFD6B5">
                                    <androidx.cardview.widget.CardView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        app:contentPadding="20dp"
                                        app:cardElevation="2dp"
                                        app:cardCornerRadius="100dp"
                                        app:cardBackgroundColor="#FFE6A5">
                                    </androidx.cardview.widget.CardView>
                                </androidx.cardview.widget.CardView>
                            </androidx.cardview.widget.CardView>
                        </androidx.cardview.widget.CardView>
                    </androidx.cardview.widget.CardView>
                </androidx.cardview.widget.CardView>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:contentPadding="15dp"
                app:cardElevation="2dp"
                app:cardCornerRadius="100dp"
                app:cardBackgroundColor="#CFA6F5">
                <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:contentPadding="15dp"
                    app:cardElevation="10dp"
                    app:cardCornerRadius="100dp"
                    app:cardBackgroundColor="#FFA6F5">
                    <androidx.cardview.widget.CardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:contentPadding="20dp"
                        app:cardElevation="2dp"
                        app:cardCornerRadius="100dp"
                        app:cardBackgroundColor="#FFB6E5">
                        <androidx.cardview.widget.CardView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:contentPadding="20dp"
                            app:cardElevation="2dp"
                            app:cardCornerRadius="100dp"
                            app:cardBackgroundColor="#FFC6D5">
                            <androidx.cardview.widget.CardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:contentPadding="20dp"
                                app:cardElevation="2dp"
                                app:cardCornerRadius="100dp"
                                app:cardBackgroundColor="#FFD6C5">
                                <androidx.cardview.widget.CardView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:contentPadding="20dp"
                                    app:cardElevation="2dp"
                                    app:cardCornerRadius="100dp"
                                    app:cardBackgroundColor="#FFD6B5">
                                    <androidx.cardview.widget.CardView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        app:contentPadding="20dp"
                                        app:cardElevation="2dp"
                                        app:cardCornerRadius="100dp"
                                        app:cardBackgroundColor="#FFE6A5">
                                    </androidx.cardview.widget.CardView>
                                </androidx.cardview.widget.CardView>
                            </androidx.cardview.widget.CardView>
                        </androidx.cardview.widget.CardView>
                    </androidx.cardview.widget.CardView>
                </androidx.cardview.widget.CardView>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
